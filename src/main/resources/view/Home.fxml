<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.HomeController">
   <children>
      <VBox alignment="CENTER" spacing="50.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="nuovaPartita" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#nuovaPartita" text="Nuova Partita" />
            <Button fx:id="carica" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#caricaPartita" text="Carica Partita" />
         </children>
      </VBox>
   </children>
</AnchorPane>
